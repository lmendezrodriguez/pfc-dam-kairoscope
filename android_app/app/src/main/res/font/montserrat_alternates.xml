    <font
        android:font="@font/montserratalternates_thin"
        android:fontStyle="normal"
        android:fontWeight="100"
        app:font="@font/montserratalternates_thin"
        app:fontStyle="normal"
        app:fontWeight="100" />

    <!-- Thin Italic -->
    <font
        android:font="@font/montserratalternates_thinitalic"
        android:fontStyle="italic"
        android:fontWeight="100"
        app:font="@font/montserratalternates_thinitalic"
        app:fontStyle="italic"
        app:fontWeight="100" />

    <!-- Black -->
    <font
        android:font="@font/montserratalternates_black"
        android:fontStyle="normal"
        android:fontWeight="900"
        app:font="@font/montserratalternates_black"
        app:fontStyle="normal"
        app:fontWeight="900" />

    <!-- Black Italic -->
    <font
        android:font="@font/montserratalternates_blackitalic"
        android:fontStyle="italic"
        android:fontWeight="900"
        app:font="@font/montserratalternates_blackitalic"
